#include <stdio.h>
#include <stdlib.h>

int main()
{
    double largura, comprimento, valor, area, preco;

    printf("Digite da largura do terreno: ");
    scanf("%lf", &largura);

    printf("Digite da comprimento do terreno: ");
    scanf("%lf", &comprimento);

    printf("Digite da valor do metro do terreno: ");
    scanf("%lf", &valor);

    area = largura * comprimento;
    preco = area * valor;

    printf("Area do terreno = %.2lf\n", area);
    printf("Area do preco terreno = %.2lf\n", preco);

    return 0;
}
